{
  "name": "fitness.admin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "lodash": "^4.17.5",
    "material-ui": "^0.20.0",
    "moment": "^2.22.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-relay": "^1.4.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "react-tap-event-plugin": "^3.0.2",
    "relay-runtime": "^1.4.1"
  },
  "graphql": {
    "request": {
      "url": "https://fitness-graphql-relay.herokuapp.com/graphql"
    }
  },
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "relay": "relay-compiler --src ./src --schema ./src/schema.graphql",
    "build": "webpack --mode production",
    "update-schema": "get-graphql-schema https://fitness-graphql-relay.herokuapp.com/graphql > ./src/schema.graphql",
    "update-schema:local": "get-graphql-schema http://localhost:8080/graphql > ./src/schema.graphql"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-relay": "^1.5.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "relay-compiler": "^1.4.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  }
}
